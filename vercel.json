{
    "framework": "vite",
    "buildCommand": "pnpm build",
    "outputDirectory": "dist",
    "installCommand": "pnpm install",
    "functions": {
        "api/search.ts": {
            "memory": 256,
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}